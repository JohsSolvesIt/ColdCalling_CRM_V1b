{
  "manifest_version": 3,
  "name": "DEBUG Realtor Extractor",
  "version": "1.0",
  "description": "Minimal debug version",
  "permissions": ["activeTab"],
  "host_permissions": ["*://*.realtor.com/*"],
  "content_scripts": [
    {
      "matches": ["*://*.realtor.com/*"],
      "js": ["debug-content.js"],
      "run_at": "document_idle"
    }
  ]
}
