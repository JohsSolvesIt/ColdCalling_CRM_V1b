            line-height: 1.6;
            margin-bottom: var(--spacing-component);
            color: var(--text-primary);
            opacity: 0.9;
            max-width: 500px;
        }
        
        .inked-hero-actions {
            display: flex;
            gap: 20px;
            margin-bottom: var(--spacing-component);
        }
        
        .inked-hero-image {
            display: flex;
            flex-direction: column;
            align-items: center;
            gap: 40px;
        }
        
        .inked-agent-photo {
            width: 200px;
            height: 200px;
            border-radius: 50%;
            overflow: hidden;
            border: 4px solid var(--text-primary);
            box-shadow: 0 20px 40px rgba(0, 0, 0, 0.3);
            background: var(--card-color);
            display: flex;
            align-items: center;
            justify-content: center;
            margin-bottom: 30px;
        }
        
        .inked-agent-photo img {
            width: 100%;
            height: 100%;
            object-fit: cover;
        }
        
        /* Fallback when no image loads */
        .inked-agent-photo.no-image::before {
            content: "👤";
            font-size: 80px;
            color: var(--text-accent);
        }
        
        .inked-hero-stats {
            display: flex;
            justify-content: center;
            gap: 20px;
            width: 100%;
            max-width: 800px;
            flex-wrap: wrap;
            margin-top: 40px;
        }
        
        .inked-stat-card {
            background: var(--card-color);
            padding: 25px 20px;
            border-radius: var(--border-radius-large);
            text-align: center;
            backdrop-filter: blur(10px);
            box-shadow: 0 8px 25px rgba(0, 0, 0, 0.3);
            min-height: 100px;
            min-width: 160px;
            display: flex;
            flex-direction: column;
            justify-content: center;
            border: 2px solid var(--border-color);
            flex: 1;
        }
        
        .inked-stat-value {
            font-size: 38px;
            font-weight: 700;
            color: var(--text-primary);
            margin-bottom: 8px;
            line-height: 1.1;
        }
        
        .inked-stat-label {
            font-size: 13px;
            font-weight: 600;
            color: var(--text-accent);
            text-transform: uppercase;
            letter-spacing: 1px;
        }        /* Mobile Hero Adjustments */
        @media (max-width: 768px) {
            .inked-hero-search {
                flex-direction: column;
                gap: 8px;
            }
            
            .inked-hero-search-button {
                border-radius: var(--border-radius-medium);
            }
        }
        
        /* Statistics Section */
        .inked-stats {
            background: var(--surface-color);
            padding: var(--spacing-section) 0;
        }
        
        .inked-stats-container {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            gap: var(--spacing-component);
            max-width: 1400px;
            margin: 0 auto;
            padding: 0 24px;
        }
        
        .inked-stats-item {
            background: var(--background-color);
            padding: var(--spacing-component);
            border-radius: var(--border-radius-large);
            text-align: center;
            box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);
            transition: transform var(--animation-normal);
        }
        
        .inked-stats-item:hover {
            transform: translateY(-5px);
        }
        
        .inked-stats-number {
            font-size: clamp(2.5rem, 5vw, 4rem);
            font-weight: 800;
            color: var(--primary-color);
            margin-bottom: 10px;
        }
        
        .inked-stats-label {
            font-size: 18px;
            color: var(--text-secondary);
            margin-bottom: var(--spacing-element);
        }
        
        .inked-stats-cta {
            display: inline-block;
            background: var(--accent-color);
            color: var(--text-inverse);
            padding: 12px 24px;
            border-radius: var(--border-radius-medium);
            text-decoration: none;
            font-weight: 600;
            transition: all var(--animation-normal);
        }
        
        .inked-stats-cta:hover {
            background: #059669;
            transform: translateY(-2px);
        }
        
        /* Properties Section */
        .inked-properties {
            padding: var(--spacing-section) 0;
            background: var(--background-color);
        }
        
        .inked-properties-container {
            max-width: 1400px;
            margin: 0 auto;
            padding: 0 24px;
        }
        
        .inked-properties-header {
            text-align: center;
            margin-bottom: var(--spacing-component);
        }
        
        .inked-properties-title {
            color: var(--text-primary);
            margin-bottom: var(--spacing-element);
        }
        
        .inked-properties-subtitle {
            font-size: 18px;
            color: var(--text-secondary);
            max-width: 600px;
            margin: 0 auto;
        }
        
        .inked-properties-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(350px, 1fr));
            gap: var(--spacing-card);
        }
        
        /* Property Card Styles - IMPROVED TEXT VISIBILITY */
        .property-card {
            background: var(--card-color);
            border-radius: var(--border-radius-large);
            overflow: hidden;
            box-shadow: 0 4px 20px rgba(0, 0, 0, 0.2);
            transition: transform var(--animation-normal);
            border: 2px solid var(--border-color);
        }
        
        .property-card:hover {
            transform: translateY(-5px);
            box-shadow: 0 8px 30px rgba(0, 0, 0, 0.3);
        }
        
        .property-image {
            width: 100%;
            height: 250px;
            object-fit: cover;
        }
        
        .property-content {
            padding: var(--spacing-card);
        }
        
        .property-price {
            font-size: 24px;
            font-weight: 700;
            color: var(--text-accent);
            margin-bottom: 8px;
        }
        
        .property-address {
            font-size: 18px;
            color: var(--text-primary);
            margin-bottom: 16px;
            font-weight: 600;
        }
        
        .property-details {
            display: flex;
            gap: 20px;
            color: var(--text-primary);
            font-size: 14px;
            font-weight: 500;
        }
        
        /* Testimonials Section - FIX CONTAINER OVERFLOW */
        .inked-testimonials {
            background: var(--secondary-color);
            padding: var(--spacing-section) 0;
            overflow: hidden; /* PREVENT SPILLOVER */
        }
        
        .inked-testimonials-header {
            text-align: center;
            margin-bottom: var(--spacing-component);
            max-width: 1200px;
            margin-left: auto;
            margin-right: auto;
            padding: 0 40px;
        }
        
        .inked-testimonials-title {
            font-family: var(--font-heading);
            font-size: clamp(2rem, 5vw, 3rem);
            font-weight: 700;
            color: var(--text-primary);
            margin-bottom: var(--spacing-element);
        }
        
        .inked-testimonials-subtitle {
            font-size: 1.125rem;
            color: var(--text-secondary);
            max-width: 600px;
            margin: 0 auto;
        }
        
        .testimonial-card {
            background: var(--card-color);
            padding: var(--spacing-component);
            border-radius: var(--border-radius-large);
            box-shadow: 0 4px 20px rgba(0, 0, 0, 0.2);
            margin-bottom: var(--spacing-card);
            border: 2px solid var(--border-color);
            max-width: 1200px; /* PREVENT OVERFLOW */
            margin-left: auto;
            margin-right: auto;
            overflow: hidden; /* PREVENT TEXT SPILLOVER */
            word-wrap: break-word; /* BREAK LONG WORDS */
        }
        
        .testimonial-text {
            font-style: italic;
            font-size: 18px;
            line-height: 1.6;
            margin-bottom: var(--spacing-element);
            color: var(--text-primary); /* WHITE TEXT FOR TESTIMONIALS */
            font-weight: 500;
        }
        
        .testimonial-author {
            font-weight: 700;
            color: var(--text-accent);
            font-size: 16px;
        }
        
        /* Generated Testimonial Styles - FIX DARK GREY TEXT AND CONTAINER OVERFLOW */
        .testimonial-quote {
            font-style: italic;
            font-size: 18px;
            line-height: 1.6;
            margin-bottom: var(--spacing-element);
            color: var(--text-primary) !important; /* WHITE TEXT FOR QUOTES */
            font-weight: 500;
            max-width: 100% !important; /* PREVENT OVERFLOW */
            overflow: hidden !important; /* PREVENT SPILLOVER */
            word-wrap: break-word !important; /* BREAK LONG WORDS */
            hyphens: auto !important; /* AUTO HYPHENATION */
        }
        
        .testimonial-content {
            max-width: 1200px !important; /* CONSTRAIN CONTAINER */
            margin: 0 auto !important; /* CENTER CONTAINER */
            padding: 0 40px !important; /* ADD SIDE PADDING */
            overflow: hidden !important; /* PREVENT SPILLOVER */
        }
        
        .testimonial-content .testimonial-quote {
            color: var(--text-primary) !important; /* WHITE TEXT FOR FALLBACK QUOTES */
        }
        
        .testimonial-slide {
            max-width: 1200px !important; /* CONSTRAIN SLIDES */
            margin: 0 auto !important; /* CENTER SLIDES */
            padding: 0 40px !important; /* ADD SIDE PADDING */
            overflow: hidden !important; /* PREVENT SPILLOVER */
        }
        
        .testimonial-slide .testimonial-quote {
            color: var(--text-primary) !important; /* WHITE TEXT FOR CAROUSEL QUOTES */
        }
        
        .testimonial-carousel {
            max-width: 1200px !important; /* CONSTRAIN CAROUSEL */
            margin: 0 auto !important; /* CENTER CAROUSEL */
            overflow: hidden !important; /* PREVENT SPILLOVER */
        }
        
        .testimonial-content .testimonial-author,
        .testimonial-slide .testimonial-author {
            font-weight: 700;
            color: var(--text-accent) !important;
            font-size: 16px;
            text-transform: uppercase;
        }
        
        /* Contact Section */
        .inked-contact {
            background: var(--background-color);
            padding: var(--spacing-section) 0;
            text-align: center;
        }
        
        .contact-info {
            margin: var(--spacing-component) 0;
        }
        
        .contact-info p {
            font-size: 18px;
            margin-bottom: 12px;
        }
        
        /* Animations */
        @keyframes fadeInUp {
            from {
                opacity: 0;
                transform: translateY(30px);
            }
            to {
                opacity: 1;
                transform: translateY(0);
            }
        }
        
        @keyframes fadeIn {
            from { opacity: 0; }
            to { opacity: 1; }
        }
        
        /* Responsive Design */
        @media (max-width: 1200px) {
            .inked-properties-grid {
                grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            }
        }
        
        @media (max-width: 768px) {
            :root {
                --spacing-section: 80px;
                --spacing-component: 30px;
            }
            
            .inked-stats-container {
                grid-template-columns: 1fr;
            }
            
            .inked-properties-grid {
                grid-template-columns: 1fr;
            }
        }
        
        @media (max-width: 480px) {
            .inked-container {
                padding: 0 12px;
            }
            
            .inked-hero-content {
                padding: 0 12px;
            }
        }
        
        /* Utility Classes */
        .text-center { text-align: center; }
        .text-left { text-align: left; }
        .text-right { text-align: right; }
        
        .mb-1 { margin-bottom: 8px; }
        .mb-2 { margin-bottom: 16px; }
        .mb-3 { margin-bottom: 24px; }
        .mb-4 { margin-bottom: 32px; }
        
        .mt-1 { margin-top: 8px; }
        .mt-2 { margin-top: 16px; }
        .mt-3 { margin-top: 24px; }
        .mt-4 { margin-top: 32px; }
        
        /* Accessibility */
        @media (prefers-reduced-motion: reduce) {
            * {
                animation-duration: 0.01ms !important;
                animation-iteration-count: 1 !important;
                transition-duration: 0.01ms !important;
            }
        }
        
        /* Focus Styles */
        .inked-btn:focus,
        .inked-nav-link:focus,
        .inked-hero-search-input:focus,
        .inked-hero-search-button:focus {
            outline: 2px solid var(--accent-color);
            outline-offset: 2px;
        }
        
        /* Mobile Hero Responsiveness - FIXED FOR NEW LAYOUT */
        @media (max-width: 768px) {
            .inked-hero-title {
                font-size: clamp(1.8rem, 5vw, 2.5rem);
            }
            
            .inked-hero-actions {
                flex-direction: column;
                gap: 15px;
                margin-bottom: 30px;
            }
            
            .inked-btn {
                min-width: auto;
                padding: 16px 32px;
                font-size: 14px;
                width: 100%;
                max-width: 300px;
            }
            
            .inked-hero-stats {
                flex-direction: column;
                gap: 15px;
                width: 100%;
                max-width: 300px;
            }
            
            .inked-stat-card {
                min-height: 80px;
                padding: 20px 15px;
                min-width: auto;
            }
            
            .inked-stat-value {
                font-size: 24px;
            }
            
            /* TESTIMONIALS MOBILE FIX */
            .testimonial-content,
            .testimonial-slide,
            .testimonial-carousel {
                padding: 0 20px !important;
            }
            
            .inked-testimonials-header {
                padding: 0 20px;
            }
        }
    </style>
</head>
<body>
    <!-- REQUIRED: Compatible navigation structure -->
    <nav class="inked-navigation">
        <div class="inked-nav-container">
            <a href="#" class="inked-nav-logo">{{AGENT_NAME}}</a>
            <ul class="inked-nav-menu">
                <li><a href="#properties" class="inked-nav-link">Properties</a></li>
                <li><a href="#about" class="inked-nav-link">About</a></li>
                <li><a href="#testimonials" class="inked-nav-link">Reviews</a></li>
                <li><a href="#contact" class="inked-nav-cta">Contact Me</a></li>
            </ul>
        </div>
    </nav>

    <!-- REQUIRED: Hero section with ALL generator placeholders -->
    <section class="inked-hero" style="background-image: {{HERO_BACKGROUND_IMAGE}}; filter: {{HERO_BLUR}};">
        <div class="inked-hero-overlay" style="background: {{HERO_OVERLAY}};"></div>
        <div class="inked-hero-content">
            <!-- CORRECT ORDER: PROFILE PICTURE FIRST -->
            <div class="inked-agent-photo">
                <img src="{{AGENT_PHOTO}}" alt="{{AGENT_NAME}}" onerror="this.style.display='none'; this.parentNode.classList.add('no-image');">
            </div>
            
            <!-- THEN NAME AND TITLE -->
            <div class="inked-hero-text">
                <h1 class="inked-hero-title">{{AGENT_NAME}}</h1>
                <h2 class="inked-hero-subtitle">{{AGENT_TITLE}}</h2>
                <p class="inked-hero-description">{{AGENT_BIO}}</p>
            </div>
            
            <!-- THEN BUTTONS -->
            <div class="inked-hero-actions">
                <a href="#portfolio" class="inked-btn inked-btn-primary">SEARCH PROPERTIES</a>
                <a href="#contact" class="inked-btn inked-btn-secondary">HOME VALUATION</a>
            </div>
            
            <!-- THEN STATS -->
            <div class="inked-hero-stats">
                <div class="inked-stat-card">
                    <div class="inked-stat-value">{{TOTAL_PROPERTIES}}</div>
                    <div class="inked-stat-label">Properties Sold</div>
                </div>
                <div class="inked-stat-card">
                    <div class="inked-stat-value">{{YEARS_EXPERIENCE}}</div>
                    <div class="inked-stat-label">Years Experience</div>
                </div>
                <div class="inked-stat-card">
                    <div class="inked-stat-value">{{PRICE_RANGE}}</div>
                    <div class="inked-stat-label">Price Range</div>
                </div>
            </div>
        </div>
    </section>

    <!-- REQUIRED: Statistics section -->
    <section class="inked-stats">
        <div class="inked-stats-container">
            <div class="inked-stats-item">
                <div class="inked-stats-number">{{CLIENT_SATISFACTION}}</div>
                <div class="inked-stats-label">Client Satisfaction</div>
                <a href="#contact" class="inked-stats-cta">Contact Us</a>
            </div>
            <div class="inked-stats-item">
                <div class="inked-stats-number">{{SALES_VOLUME}}</div>
                <div class="inked-stats-label">Sales Volume</div>
                <a href="#testimonials" class="inked-stats-cta">See Reviews</a>
            </div>
        </div>
    </section>

    <!-- REQUIRED: Properties section with generator placeholder -->
    <section class="inked-properties" id="properties">
        <div class="inked-properties-container">
            <div class="inked-properties-header">
                <h2 class="inked-properties-title">Featured Properties</h2>
                <p class="inked-properties-subtitle">Discover exceptional homes in prime locations</p>
            </div>
            <div class="inked-properties-grid">
                {{PORTFOLIO_GRID}}
            </div>
        </div>
    </section>

    <!-- REQUIRED: Testimonials section -->
        <!-- REQUIRED: Testimonials section -->
    <section class="inked-testimonials" id="testimonials">
        <div class="inked-container">
            <div class="inked-testimonials-header">
                <h2 class="inked-testimonials-title">Client Reviews</h2>
                <p class="inked-testimonials-subtitle">See what our clients say about their experience</p>
            </div>
            {{TESTIMONIALS_CONTENT}}
        </div>
    </section>

    <!-- REQUIRED: Contact section -->
    <section class="inked-contact" id="contact">
        <div class="inked-container">
            <h2>Contact {{AGENT_NAME}}</h2>
            <div class="contact-info">
                <p><strong>Phone:</strong> {{AGENT_PHONE}}</p>
                <p><strong>Email:</strong> {{AGENT_EMAIL}}</p>
                <p><strong>Company:</strong> {{COMPANY_NAME}}</p>
            </div>
            <a href="tel:{{AGENT_PHONE}}" class="inked-btn inked-btn-primary">Call Now</a>
        </div>
    </section>

    <!-- Optional JavaScript for enhanced interactions -->
    <script>
        // Smooth scrolling for navigation links
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    target.scrollIntoView({
                        behavior: 'smooth',
                        block: 'start'
                    });
                }
            });
        });

        // Navigation background on scroll
        window.addEventListener('scroll', function() {
            const nav = document.querySelector('.inked-navigation');
            if (window.scrollY > 100) {
                nav.style.background = 'rgba(255, 255, 255, 0.98)';
            } else {
                nav.style.background = 'rgba(255, 255, 255, 0.95)';
            }
        });

        // Hero search functionality
        const searchButton = document.querySelector('.inked-hero-search-button');
        const searchInput = document.querySelector('.inked-hero-search-input');
        
        if (searchButton && searchInput) {
            searchButton.addEventListener('click', function() {
                const searchValue = searchInput.value.trim();
                if (searchValue) {
                    // Scroll to properties section
                    document.querySelector('#properties').scrollIntoView({
                        behavior: 'smooth'
                    });
                }
            });
            
            searchInput.addEventListener('keypress', function(e) {
                if (e.key === 'Enter') {
                    searchButton.click();
                }
            });
        }
    </script>
</body>
</html>
