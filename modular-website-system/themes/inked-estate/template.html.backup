    <style>
        /* === INKED ESTATE THEME - COMPLETE CSS IMPLEMENTATION === */
        
        /* CSS Variables - DARKER SATURATED BACKGROUNDS + BRIGHTER FOREGROUND */
        :root {
            /* DARKER SATURATED BACKGROUNDS */
            --primary-color: #1a3d1a;        /* Very dark saturated forest green */
            --secondary-color: #2d4a2d;      /* Dark forest green */
            --accent-color: #4a6b4a;         /* Medium forest green */
            --success-color: #6b8e6b;        /* Sage green for success */
            
            /* Base Colors - ALL DARK */
            --background-color: #1a3d1a;     /* Very dark forest green background */
            --surface-color: #2d4a2d;        /* Dark forest green cards/sections */
            --card-color: #3d5a3d;           /* Medium dark for cards */
            --dark-color: #0d1a0d;          /* Almost black forest green */
            
            /* BRIGHTER LESS SATURATED FOREGROUND */
            --text-primary: #f0f8f0;         /* Very bright, slightly green-tinted white */
            --text-secondary: #d8e8d8;       /* Bright light sage */
            --text-muted: #c0d0c0;           /* Medium light sage */
            --text-inverse: #1a3d1a;         /* Dark text for light surfaces */
            --text-accent: #a8c8a8;          /* Bright desaturated sage accent */
            
            /* Navigation & Button Colors - Blend with background */
            --nav-background: #1a3d1a;       /* Same as primary */
            --nav-button: #4a6b4a;           /* Medium green for contrast */
            --nav-button-hover: #6b8e6b;     /* Lighter on hover */
            
            /* Borders & Surfaces */
            --border-color: #4a6b4a;         /* Medium forest borders */
            --border-radius-small: 6px;
            --border-radius-medium: 8px;
            --border-radius-large: 12px;
            --border-radius-xlarge: 16px;
            
            /* Overlays */
            --overlay-dark: rgba(26, 61, 26, 0.95);
            --overlay-light: rgba(240, 248, 240, 0.95);
            --overlay-green: rgba(74, 107, 74, 0.4);
            
            /* Typography */
            --font-heading: 'Playfair Display', serif;
            --font-body: 'Inter', -apple-system, BlinkMacSystemFont, sans-serif;
            
            /* Spacing */
            --spacing-section: 120px;
            --spacing-component: 40px;
            --spacing-card: 30px;
            --spacing-element: 20px;
            
            /* Animations */
            --animation-fast: 0.15s;
            --animation-normal: 0.3s;
            --animation-slow: 0.5s;
            --animation-easing: cubic-bezier(0.4, 0, 0.2, 1);
        }
        
        /* Reset & Base Styles */
        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }
        
        html {
            scroll-behavior: smooth;
        }
        
        body {
            font-family: var(--font-body);
            font-weight: 400;
            line-height: 1.6;
            color: var(--text-primary);
            background-color: var(--background-color);
            overflow-x: hidden;
        }
        
        /* Typography Hierarchy */
        h1, h2, h3, h4, h5, h6 {
            font-family: var(--font-heading);
            font-weight: 700;
            line-height: 1.2;
            letter-spacing: 2px;
            margin-bottom: var(--spacing-element);
        }
        
        h1 { font-size: clamp(2.5rem, 5vw, 4rem); }
        h2 { font-size: clamp(2rem, 4vw, 3rem); }
        h3 { font-size: clamp(1.5rem, 3vw, 2.25rem); }
        h4 { font-size: clamp(1.25rem, 2.5vw, 1.75rem); }
        h5 { font-size: clamp(1.125rem, 2vw, 1.5rem); }
        h6 { font-size: clamp(1rem, 1.5vw, 1.25rem); }
        
        p {
            margin-bottom: var(--spacing-element);
            color: var(--text-secondary);
        }
        
        /* Container System */
        .inked-container {
            max-width: 1400px;
            margin: 0 auto;
            padding: 0 24px;
        }
        
        @media (max-width: 768px) {
            .inked-container {
                padding: 0 16px;
            }
        }
        
        /* Button System */
        .inked-btn {
            display: inline-flex;
            align-items: center;
            justify-content: center;
            padding: 16px 32px;
            border: none;
            border-radius: var(--border-radius-medium);
            font-family: var(--font-body);
            font-weight: 600;
            font-size: 16px;
            text-decoration: none;
            cursor: pointer;
            transition: all var(--animation-normal) var(--animation-easing);
            position: relative;
            overflow: hidden;
        }
        
        .inked-btn-primary {
            background: var(--success-color);
            color: var(--text-primary);
            box-shadow: 0 4px 14px rgba(107, 142, 107, 0.4);
            font-weight: 700;
            border: 2px solid var(--nav-button-hover);
        }
        
        .inked-btn-primary:hover {
            background: var(--nav-button-hover);
            color: var(--text-primary);
            transform: translateY(-2px);
            box-shadow: 0 8px 25px rgba(107, 142, 107, 0.6);
        }
        
        .inked-btn-secondary {
            background: transparent;
            color: var(--text-primary);
            border: 2px solid var(--text-primary);
        }
        
        .inked-btn-secondary:hover {
            background: var(--text-primary);
            color: var(--background-color);
            transform: translateY(-2px);
            box-shadow: 0 8px 25px rgba(240, 248, 240, 0.3);
        }
        
        /* Navigation - FIXED FORMATTING AND BOUNDARIES */
        .inked-navigation {
            position: fixed;
            top: 0;
            left: 0;
            right: 0;
            background: var(--background-color) !important;
            border-bottom: 2px solid var(--border-color); /* THICKER BORDER FOR BETTER DEFINITION */
            z-index: 1000;
            transition: all var(--animation-normal);
            backdrop-filter: blur(10px);
            -webkit-backdrop-filter: blur(10px);
            box-shadow: 0 4px 15px rgba(0, 0, 0, 0.4); /* STRONGER SHADOW FOR SEPARATION */
            overflow: hidden; /* PREVENT COLOR OVERFILL */
        }
        
        .inked-nav-container {
            display: flex;
            align-items: center;
            justify-content: space-between;
            max-width: 1800px; /* MAXIMUM WIDTH TO MATCH HERO CONTENT */
            margin: 0 auto;
            padding: 0 40px;
            height: 80px;
            width: 100%;
            box-sizing: border-box;
            position: relative; /* ENSURE PROPER BOUNDARY CONTAINMENT */
        }
        
        .inked-nav-logo {
            font-family: var(--font-heading);
            font-size: 24px;
            font-weight: 700;
            color: var(--text-primary);
            text-decoration: none;
            letter-spacing: 2px;
            text-transform: uppercase;
            white-space: nowrap; /* PREVENT LOGO WRAPPING */
        }
        
        .inked-nav-menu {
            display: flex;
            list-style: none;
            gap: 30px; /* REDUCED GAP FOR BETTER MOBILE FIT */
            align-items: center;
            margin: 0;
            padding: 0;
            flex-wrap: wrap; /* ALLOW WRAPPING IF NEEDED */
        }
        
        .inked-nav-link {
            color: var(--text-secondary);
            text-decoration: none;
            font-weight: 500;
            transition: color var(--animation-fast);
            position: relative;
            padding: 8px 16px;
            border-radius: var(--border-radius-medium);
            background: var(--nav-button);
            white-space: nowrap; /* PREVENT LINK TEXT WRAPPING */
        }
        
        .inked-nav-link:hover {
            color: var(--text-primary);
            background: var(--nav-button-hover);
        }
        
        .inked-nav-link::after {
            content: '';
            position: absolute;
            bottom: -4px;
            left: 0;
            width: 0;
            height: 2px;
            background: var(--text-accent);
            transition: width var(--animation-normal);
        }        .inked-nav-link:hover::after {
            width: 100%;
        }
        
        .inked-nav-cta {
            background: var(--nav-button);
            color: var(--text-primary);
            padding: 12px 24px;
            border-radius: var(--border-radius-medium);
            text-decoration: none;
            font-weight: 600;
            transition: all var(--animation-normal);
            text-transform: uppercase;
            letter-spacing: 1px;
            font-size: 14px;
        }
        
        .inked-nav-cta:hover {
            background: var(--nav-button-hover);
            transform: translateY(-2px);
            box-shadow: 0 4px 12px rgba(74, 107, 74, 0.3);
        }
        
        /* Mobile Navigation - HIDE BUTTONS ON MOBILE */
        @media (max-width: 1024px) {
            .inked-nav-menu {
                gap: 20px;
            }
            
            .inked-nav-link {
                padding: 6px 12px;
                font-size: 14px;
            }
            
            .inked-nav-cta {
                padding: 10px 18px;
                font-size: 12px;
            }
        }
        
        @media (max-width: 768px) {
            .inked-nav-menu {
                display: none; /* HIDE ALL NAVIGATION BUTTONS ON MOBILE */
            }
            
            .inked-nav-container {
                padding: 0 16px;
                justify-content: center; /* Center the logo */
                height: 60px; /* Smaller height for mobile */
            }
            
            .inked-nav-logo {
                margin-bottom: 0;
            }
        }
        
        @media (max-width: 480px) {
            .inked-nav-container {
                height: 50px; /* Even smaller on very small screens */
            }
            
            .inked-nav-logo {
                font-size: 20px; /* Smaller logo text */
            }
        }
        
        /* Hero Section - COMPLETELY REDESIGNED FOR PROPER CONTAINMENT */
        .inked-hero {
            position: relative;
            min-height: 100vh;
            width: 100%;
            display: flex;
            align-items: center;
            justify-content: center;
            background: var(--primary-color);
            background-size: cover !important;
            background-position: center !important;
            background-repeat: no-repeat !important;
            margin-top: 100px; /* INCREASED SPACING FROM NAVBAR */
            overflow: hidden; /* PREVENT ANY OVERFLOW */
        }
        
        .inked-hero-overlay {
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background: var(--overlay-dark);
            z-index: 1;
        }
        
        /* HERO CONTAINER - PROPERLY CONTAINED AND CENTERED */
        .inked-hero-content {
            position: relative;
            z-index: 2;
            width: 100%;
            max-width: 1800px; /* MAXIMUM WIDTH FOR ALL DEVICES */
            margin: 0 auto;
            padding: 80px 60px; /* INCREASED DESKTOP PADDING */
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            text-align: center;
            box-sizing: border-box;
            min-height: calc(100vh - 180px); /* ACCOUNT FOR INCREASED NAVBAR SPACING */
        }
        
        /* AGENT PHOTO - PROPERLY CONTAINED */
        .inked-agent-photo {
            width: 120px;
            height: 120px;
            margin: 0 auto 30px auto;
            border-radius: 50%;
            overflow: hidden;
            border: 4px solid var(--inked-accent-color);
            box-shadow: 0 8px 25px rgba(0, 0, 0, 0.4);
            flex-shrink: 0; /* PREVENT SHRINKING */
        }
        
        .inked-agent-photo img {
            width: 100%;
            height: 100%;
            object-fit: cover;
            display: block;
        }
        
        /* HERO TEXT CONTAINER */
        .inked-hero-text {
            width: 100%;
            max-width: 800px;
            margin: 0 auto 40px auto;
        }
        
        /* REMOVE ALL DEFAULT H1/H2 STYLING THAT COULD CAUSE LINES */
        .inked-hero-text h1,
        .inked-hero-text h2,
        .inked-hero-title,
        .inked-hero-subtitle {
            background: none !important;
