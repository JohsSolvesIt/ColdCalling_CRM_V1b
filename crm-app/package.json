{
  "name": "crm-app",
  "version": "1.0.0",
  "description": "CSV-to-CRM application with PostgreSQL database",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently --kill-others --prefix \"[{name}]\" --names \"Backend,Frontend\" \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "install-all": "npm install && cd client && npm install",
    "setup": "npm run install-all && npm run build",
    "netlify-build": "npm run install-all && npm run build",
    "vvebjs:generate": "cd ../vvebjs && npm run generate-realtors",
    "vvebjs:serve": "cd ../vvebjs && npm run serve-realtor",
    "vvebjs:dev": "npm run vvebjs:generate && npm run vvebjs:serve"
  },
  "dependencies": {
    "archiver": "^5.3.2",
    "axios": "^1.11.0",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "express": "^4.18.2",
    "form-data": "^4.0.4",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "path": "^0.12.7",
    "pg": "^8.16.3",
    "serverless-http": "^3.2.0",
    "uuid": "^9.0.1"
  },
  "keywords": [
    "crm",
    "csv",
    "postgresql",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
